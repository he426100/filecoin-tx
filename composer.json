    "type": "library",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "skleeschulte/base32": "^0.0.2",
        "simplito/elliptic-php": "^1.0",
        "deemru/blake2b": "^1.0"
    },
    "autoload": {
